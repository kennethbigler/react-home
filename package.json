{
  "homepage": "http://kennethbigler.github.io/",
  "name": "react-home",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.1.5",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "copy-to-clipboard": "3.3.1",
    "graphql-tag": "2.11.0",
    "localforage": "1.9.0",
    "lodash": "4.17.20",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-newline-to-break": "1.0.6",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "react-simple-maps": "2.1.2",
    "recharts": "1.8.5",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@types/jest": "26.0.14",
    "@types/lodash": "4.14.161",
    "@types/material-ui": "0.21.8",
    "@types/node": "13.13.21",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@types/react-simple-maps": "1.0.2",
    "@types/recharts": "1.8.15",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.9.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "graphql": "15.3.0",
    "react-scripts": "3.4.3",
    "source-map-explorer": "2.5.0",
    "typescript": "4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "deploy": "./scripts/deploy.sh",
    "analyze": "source-map-explorer 'docs/static/js/*.js'",
    "analyze-local": "./scripts/analyze.sh",
    "lint": "clear && eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "qlint": "clear && eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix --quiet"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "src/components/common/dark-table-cell/"
    ],
    "coverageThreshold": {
      "./src/apis/": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "./src/components/common/": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}

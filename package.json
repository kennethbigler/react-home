    "typescript": "4.9.5"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "DEBUG_PRINT_LIMIT=100000 react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:debug": "DEBUG_PRINT_LIMIT=100000 react-scripts test --coverage",
    "test:all": "react-scripts test --coverage --watchAll",
    "deploy": "./scripts/deploy.sh",
    "analyze": "source-map-explorer 'docs/static/js/*.js'",
    "analyze:local": "./scripts/analyze.sh",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:q": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix --quiet"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/service-worker.ts",
      "<rootDir>/src/serviceWorkerRegistration.ts",
      "<rootDir>/node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 85,
        "lines": 90,
        "functions": 90
      }
    },
    "coverageReporters": [
      "json",
      "text"
    ]
  },
  "keywords": [

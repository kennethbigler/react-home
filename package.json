  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "DEBUG_PRINT_LIMIT=100000 react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:coverage-debug": "DEBUG_PRINT_LIMIT=100000 react-scripts test --coverage",
    "test:coverage-all": "react-scripts test --coverage --watchAll",
    "deploy": "./scripts/deploy.sh",
    "analyze": "source-map-explorer 'docs/static/js/*.js'",
    "analyze-local": "./scripts/analyze.sh",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "qlint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix --quiet"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/components/common/dark-table-cell/",
      "<rootDir>/src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "lines": 90,
        "functions": 90
      }
    },
    "coverageReporters": [
      "json",
      "text"
    ]
  },
